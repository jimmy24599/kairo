# Environment Variables for Kairo AI Agent

# Backend API URL (where the Node.js backend server runs)
NEXT_PUBLIC_BACKEND_URL=http://localhost:3001

# WebSocket URL (for real-time communication with the backend)
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:3001

# Frontend URL (for redirects and links)
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000

# Development Server URL (for running generated projects)
NEXT_PUBLIC_DEV_SERVER_URL=http://localhost:3000

# OpenAI/OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/Kairo

# Project Root Directory
PROJECT_ROOT=/Users/ahmed/Desktop/APPS/Kairo
